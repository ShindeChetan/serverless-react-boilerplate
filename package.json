{
  "name": "serverless-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env IS_OFFLINE=1 concurrently --kill-others \"npm run start:devserver\" \"npm run start:offline\"",
    "start:devserver": "webpack-dev-server --config webpack.browser.config.js --mode development",
    "start:offline": "sls offline start",
    "build:serverless": "sls package",
    "build:browser": "webpack --config webpack.browser.config.js --mode production",
    "deploy": "sls deploy",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-stage-2": "^7.8.3",
    "aws-sdk": "^2.690.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "null-loader": "^4.0.0",
    "prettier": "^2.0.5",
    "serverless": "^1.72.0",
    "serverless-export-env": "^1.3.1",
    "serverless-offline": "^5.12.1",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-s3-deploy": "^0.9.0",
    "serverless-webpack": "^5.3.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-stats-plugin": "^0.3.1"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  }
}
